.tree
    min-height 20px
    padding 19px
    margin-bottom 20px

.tree li
    list-style-type none
    margin 0
    padding 10px 5px 0 5px
    position relative

.tree li::before, .tree li::after
    content ''
    left -20px
    position absolute
    right auto

.tree li::before
    border-left 1px solid #999
    bottom 50px
    height 100%
    top 0
    width 1px

.tree li::after
    border-top 1px solid #999
    height 20px
    top 30px
    width 25px

.tree li>.self>span
    -moz-border-radius 5px
    -webkit-border-radius 5px
    border 1px solid #999
    border-radius 5px
    display inline-block
    padding 3px 8px
    text-decoration none

.tree li.parent_li>.self>span
    cursor pointer

.tree>ul>li::before, .tree>ul>li::after
    border 0

.tree li:last-child::before
    height 30px

.tree li.parent_li>.self>span:hover, .tree li.parent_li>.self>span:hover+ul li>.self>span
    background #eee
    border 1px solid #94a0b4
    color #000

.tree
    .dl_btn_small
        padding-top 6px
        padding-bottom 6px

    .inline
        display inline

    .operator
        padding-left 20px