mixin group_thumbnail(teams,team_class,team_head)
  .groupsbox
    p.subtitle= team_head
    .groups
      each team in teams
        div.center-block(class='group '+team_class title="#{team.name}")
          a.center-block(href="/group/home/#{team._id}")
            img.center-block.group.size_56(src="#{team.logo}")
