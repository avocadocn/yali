.modal.fade#events-modal(tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true")
  .modal-dialog
    .dl-modal
      .dl-modal-header
        button.head(type="button" class="close" data-dismiss="modal" aria-hidden="true")
          img(ng-src='/img/icons/icon_close.png')
      .dl-modal-body.schedulebox
        .calendar_box
          p.pull-left.schedule_type
            button.schedule_btn.join(ng-disabled="campaignsType=='joined'" ng-click="getCampaigns('joined','modal')") 已参加
            button.schedule_btn.unjoin(ng-disabled="campaignsType=='unjoin'" ng-click="getCampaigns('unjoin','modal')") 未参加
            button.schedule_btn.all(ng-disabled="campaignsType=='all'" ng-click="getCampaigns('all','modal')") 所有
          p.calendar_nav#calendar_nav
            span(ng-class="{'icon_fold':isCalendar,'icon_unfold':!isCalendar}" ng-click="isCalendar=!isCalendar;" ng-cloak)
            a.pull-right(data-calendar-nav="next" href="javascript:void(0)") &gt;
            span.icon_calendar
            a.pull-left(data-calendar-nav="prev" href="javascript:void(0)") &lt;
            a.calendar_title(data-calendar-nav="today" href="javascript:void(0)") 
              span#calendar_title
          #calendar_view_modal.calendar_view_modal(ng-show="isCalendar")
            //- .clearfix
            //-   a.pull-right(data-calendar-view="weeks" href="javascript:void(0)" ng-show="isDayView") 返回日历
            #calendar_modal
            .calendar_tips(ng-show="isCalendar")
              span.join_sign.length_1
              label.campaign_label 没有活动
              span.join_sign.length_2
              label.campaign_label 2个及以下
              span.join_sign.length_3
              label.campaign_label 2个以上
              a.pull-right(data-calendar-nav="today" href="javascript:void(0)")
                label.today 今天
        .calendar_event_box#cal-modal-event-box