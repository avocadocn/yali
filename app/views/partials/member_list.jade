div
  span.subtitle {{"MEMBER"|translate}}
  hr
  .row
    .dl_modal_container.margin_left_10
      .groups.row
        .group(style="float:left;padding-left:20px;padding-bottom:5px" ng-repeat="member in members")
          .dl_row
            a(ng-click="userDetail($index)" data-toggle="modal" data-target="#sponsorUserDetailModel")
              img.size_80(ng-src="{{member.photo}}")
          .dl_row(style="width:80px;text-align:center")
            label {{member.nickname}}
div(class="modal fade" id="sponsorUserDetailModel" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true")
  include user_detail_modal