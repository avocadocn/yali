.dl_form_control
  display block
  width 100%
  min-height 34px
  height auto
  padding 6px 5px
  font-size 12px
  line-height 1.42857143
  color #FFFFFF
  background-color #FFFFFF
  background-image none
  border 1px solid #CCCCCC
  border-radius 0
  color #999999
  float left
  -webkit-box-shadow inset 0 1px 1px rgba(0, 0, 0, .075)
          box-shadow inset 0 1px 1px rgba(0, 0, 0, .075)
  -webkit-transition border-color ease-in-out .15s, box-shadow ease-in-out .15s
          transition border-color ease-in-out .15s, box-shadow ease-in-out .15s


.st_bar
  width 800px
  height 400px

.st_pie
  width 1170px
  height 300px

.st_pie_container
  position relative
  .st_pie_label_group
    position absolute
    width 1170px
    bottom 50px
    .st_pie_label
      float left
      width 20%
      text-align center
.no_radius
  border-radius 0

.modal-sm
  width 500px

.modal-dialog
  .box
    .box-body
      border-top none
      //- max-height 650px
      padding 0
      .modal_container
        width 100%
        float left
        clear left
        min-height 30px
        border-bottom 1px solid #f4f4f4
        padding 10px 0
        .campaign_label
          margin 0 15px
          float left
          width 100px
        .campaign_content
          float left
          color #B2B2B2
        .campaign_team
          float left
          .campaign_logo
            border-radius 5px
            height 60px
            width 60px
            margin-left 15px
            float left
          .campaign_theme, .campaign_company
            margin-top 10px
            font-size 14px
            padding 0px 10px
            width 120px
            overflow hidden
            text-overflow ellipsis
            white-space nowrap
            line-height 1
          .campaign_company
            color #666
        .campaign_member
          margin-left 20px
          .member
            height 60px
            width 60px
            border-radius 5px
            margin-left 10px
            margin-top 5px
        .campaign_member_more,.no_campaign_member
          text-align center
          font-size 14px
          color #00a65a
        .campaign_member_more
          cursor pointer
        .non_float
          float none
      .campaign_inroduce, .campaign_announce
        min-height 100px
        height auto
        .campaign_intro_content, .campaign_announce_content
          position relative
          margin-left 130px
          width 300px
          color #ccc
          word-wrap break-word
      .dl_modal_half_container
        width 205px
        float left
        .input-group-addon
          background-color #999
          border-color #bbb
          padding 0 8px
          font-size 16px
          span
            color white
      .pen-menu
        //- height 20px
        //- line-height 20px
        //- background-color #efefef
        box-shadow 0 0 0
        z-index auto
        .pen-icon
          height 25px
          line-height 25px !important
          padding 0 10px
          font-size 14px
      .markdownContainer
        outline none
        min-height 100px
        max-height 200px
        overflow-y scroll
        padding 10px
        line-height 1
        border 1px solid #eee
    .campaign_footer
      border-bottom 0
      height 60px
      padding 10px
      .campaign_modal_btn
        width 100%
        height 100%
        font-size 14px
