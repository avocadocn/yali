


// 辅助类
.clearfix
  &:before, &:after
    display table
    content " "
  &:after
    clear both


// 整体布局
.dl_main_container
  display flex
  display -webkit-flex
  #left
    .dl_aside_inner
      .media-body
        width auto
      #menu
        li.nav-header
          padding 8px 15px !important
  #content
    .outer
      padding 5px
      .inner
        border-width 5px
  .dl_content
    flex 1
    -webkit-flex 1




// view stylus 应该放在最后
@require 'view_stylus/*'